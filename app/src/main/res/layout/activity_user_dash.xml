<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/user_dash_activity_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserDashActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="user dash"
        app:layout_constraintBottom_toTopOf="@+id/postDisplay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.491" />

    <TextView
        android:id="@+id/postDisplay"
        android:layout_width="363dp"
        android:layout_height="206dp"
        android:text="posts"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/messagesDisplay"
        android:layout_width="368dp"
        android:layout_height="236dp"
        android:layout_marginTop="56dp"
        android:text="messages"
        app:layout_constraintTop_toBottomOf="@+id/postDisplay"
        tools:layout_editor_absoluteX="16dp" />


</androidx.constraintlayout.widget.ConstraintLayout>